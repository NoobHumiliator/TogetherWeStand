<?xml version="1.0"?>
<root>
	<styles>
		<include src="file://{resources}/styles/custom_game/tws_quest_affixes.css"/>
	</styles>
	<scripts>
		<include src="file://{resources}/scripts/custom_game/tws_quest_affixes.js"/>
	</scripts>
	<snippets>
		<snippet name="QuestLine">
			<Panel>
				<Panel direction="horizontal" class="Progress">
					<Panel class="Progress_Left" style="margin-bottom: 0;"/>
					<Panel class="Progress_Right" style="margin-bottom: 0;"/>
				</Panel>
				<Panel class="SliderLable">
					<Label class="SliderInfo" text=""/>
				</Panel>
			</Panel>
		</snippet>
        <snippet name="AchievementLine">
			<Panel>
				<Panel direction="horizontal" class="Progress">
					<Panel class="Progress_Left" style="margin-bottom: 0;"/>
					<Panel class="Progress_Right" style="margin-bottom: 0;"/>
				</Panel>
				<Panel class="AchievementQuestLable">
					<Label class="SliderInfo" text="#tws_quest_achievement_front"/>  <!--特殊奖励：-->
					<DOTAItemImage  itemname="" style="height:24px;width:32px; margin-top:20px;"  onmouseover="ShowItemTooltip()"
					   	  onmouseout="HideItemTooltip()"/> <!--图标：-->
					<Label class="SliderInfo" text=""/>  <!--本轮没有任何英雄阵亡：-->
				</Panel>
			</Panel>
		</snippet>


	</snippets>
	<Panel hittest="false">
		<Panel hittest="false" id="QuestMain">
			<Panel id="QuestPanel"> </Panel>
		</Panel>
	</Panel>
</root>
